<script>

    const form = document.getElementById('artistSettingsform');
    const firstname = document.getElementById('firstname');
    const lastname = document.getElementById('lastname');
    const username = document.getElementById('username');
    const mobile = document.getElementById('mobile');
    const dmobile = document.getElementById('dmobile');


    function viewImage(event) {

        if (event.target.files && event.target.files[0]) {
            const file = event.target.files[0];
            const profileValue = profile.value;

            const maxFileSize = 3145728; // 3 MB
            const maxWidth = 700;
            const maxHeight = 700;


            const img = new Image();
            img.src = URL.createObjectURL(file);
            img.onload = () => {
                if (file.size > maxFileSize) {
                    setErrorFor(profile, 'The selected image file is too big. Please choose one that is smaller than 3 MB.');
                    //  alert('The selected image file is too big. Please choose one that is smaller than 3 MB.'); //
                } else if (file.type.match('image.jpg') && file.type.match('image.jpeg')) {
                    setErrorFor(profile, 'This not jpg/jpeg file.');
                    // alert('this not jpg/jpeg file')
                } else if (file.type.match('image.*') && (img.width > maxWidth || img.height > maxHeight)) {
                    setErrorFor(profile, 'The selected image is too big. Please choose one with maximum dimensions of 700 px x 700 px.');
                    // alert(`The selected image is too big. Please choose one with maximum dimensions of ${maxWidth} px x${maxHeight} px.`);
                } else {
                    // view image onclick in image tag
                    document.getElementById('profileview').src = URL.createObjectURL(event.target.files[0])

                    event.target.nodeName === 'INPUT'
                        ? (event.target.form.querySelector("input[type='submit']").disabled = false)
                        : event.target.submit();
                }
            };
        }
    };


    function checkaInput() {

        const firstnameValue = firstname.value;
        const lastnameValue = lastname.value;
        const usernameValue = username.value;
        const mobileValue = mobile.value;
        const dmobileValue = dmobile.value;

        if (firstnameValue === '') {
            setErrorFor(firstname, 'Enter Fist name');
            return false;
        } else if (!isNaN(firstnameValue)) {
            setErrorFor(firstname, 'Enter letters only')
            return false;
        } else {
            setSuccessFor(firstname);
        }

        if (lastnameValue === '') {
            setErrorFor(lastname, 'Enter Last name');
            return false;
        } else if (!isNaN(lastnameValue)) {
            setErrorFor(lastname, 'Enter letters only')
            return false;
        } else {
            setSuccessFor(lastname);
        }

        if (usernameValue === '') {
            setErrorFor(username, 'Enter User name');
            return false;
        } else {
            setSuccessFor(username);
        }

        if (mobileValue === '') {
            setErrorFor(mobile, 'Enter complete 10 digitel number');
            return false;
        } else if (isNaN(mobileValue)) {
            setErrorFor(mobile, 'Enter Numbers only');
            return false;
        } else {
            setSuccessFor(mobile);
        }

        if (dmobileValue === '') {
            setErrorFor(dmobile, 'Enter complete 10 digitel number');
            return false;
        } else if (isNaN(dmobileValue)) {
            setErrorFor(dmobile, 'Enter Numbers only');
            return false;
        } else {
            setSuccessFor(dmobile);
        }
    }

    function setErrorFor(input, message) {
        const ArAcSettings = input.parentElement;
        const small = ArAcSettings.querySelector('small');
        ArAcSettings.className = 'ArAcSettings error';
        small.innerText = message;
    }

    function setSuccessFor(input) {
        const ArAcSettings = input.parentElement;
        ArAcSettings.className = 'ArAcSettings success';
    }
</script>


<script>
    const forms = document.getElementById('ArAcpasswordForm');
    const password11 = document.getElementById('password11');
    const password12 = document.getElementById('password12');

    function checkNewpassword() {

        const password11Value = password11.value;
        const password12Value = password12.value;



        if (password11Value === '') {
            setErrorForr(password11, 'Enter New password');
            return false;
        } else {
            setSuccessForr(password11);
        }

        if (password12Value === '') {
            setErrorForr(password12, 'Enter confiem password');
            return false;
        } else if (password11Value !== password12Value) {
            setErrorForr(password12, 'Passwords does not match');
            return false;
        } else {
            setSuccessForr(password12);
        }


    }

    function setErrorForr(input, message) {
        const ArAcPassword = input.parentElement;
        const small = ArAcPassword.querySelector('small');
        ArAcPassword.className = 'ArAcPassword notOk';
        small.innerText = message;
    }

    function setSuccessForr(input) {
        const ArAcPassword = input.parentElement;
        ArAcPassword.className = 'ArAcPassword ok';
    }
</script>



