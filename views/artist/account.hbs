                                <form action="/artist/account/change-password" method="POST" onsubmit="return checkNewpassword()"
                                    id="ArAcpasswordForm" name="passwordForm">
                                    <h3>Change password</h3>
                                    <div class="ArAcPassword">
                                        <label for="exampleInputEmail1">Email address</label>
                                        <input type="email" class="form-control" id="exampleInputEmail1"
                                            value="{{artist.EmailId}}" aria-describedby="emailHelp" name="EmailId"
                                            placeholder="Enter email" readonly>
                                        <i class="glyphicon glyphicon-remove-sign"></i>
                                        <i class="glyphicon glyphicon-ok "></i>
                                        <small>Error massage</small>
                                    </div>
                                    <div class="ArAcPassword">
                                        <label for="exampleInputPassword1">Current Password</label>
                                        <input type="password" class="form-control" name="Password1"
                                            placeholder="Password">
                                        <i class="glyphicon glyphicon-remove-sign"></i>
                                        <i class="glyphicon glyphicon-ok "></i>
                                        <small>Error massage</small>

                                    </div>
                                    <div class="ArAcPassword">
                                        <label for="exampleInputEmail1">New Password</label>
                                        <input type="password" class="form-control" id="password11" name="PasswordNew"
